<resources>

    <!-- Base application theme.
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>
    -->

    <style name="artButton" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="artButton2" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorAccent</item>
        <item name="android:textColor">#ffffff</item>
    </style>




    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ColoredButton" parent="Widget.AppCompat.Button">
        <item name="colorButtonNormal">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="RobotoTextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">sans-serif-thin</item>
    </style>

    <style name="RobotoEdiTextStyle" parent="android:Widget.EditText">
        <item name="android:fontFamily">sans-serif-thin</item>
    </style>

    <style name="RobotoButtonStyle" parent="android:Widget.Holo.Button">
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <array name="myImages">
        <item>@drawable/newbackground</item>
    </array>


</resources>
